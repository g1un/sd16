.uicheckbox.__tabstyle input[type="checkbox"],
.uiradio.__tabstyle input[type="radio"]
	& + label
		&:before
			display none
		&
			display block
			font-size 14px
			line-height 16px
			padding 12px 12px
			background #fff
			color #242424
			min-width 40px
			text-align center
			font-weight 700
			user-select none
			trans()
			transition-property color, background, border-color, opacity, filter, box-shadow, transform
		&:hover
			background #eee
	&:checked + label,
	&.-checked + label
		color #fff
		background getColor('base')

.uiradio.__tabstyle input[type="radio"]
	&:checked + label,
	&.-checked + label
		cursor default
.uicheckbox.__tabstyle input[type="checkbox"]
	&:checked + label,
	&.-checked + label
		&:hover
			background getColor('base') + 10%

.radio

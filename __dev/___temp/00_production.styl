.file-input
	position: relative;
	overflow: hidden;
	display: inline-block;
	vertical-align: top;
	margin-right: 5px;
	margin-bottom: 5px;
	cursor: default;
	input[type="file"]
		position: absolute;
		top: -20px;
		right: 1px;
		font-size: 200px;
		cursor: pointer;
		border: 0;
		width: auto;
		opacity: 0;
	&-button
		display block
		background #006298
		font-size 14px
		line-height 20px
		color #fff
		padding 10px 15px
		transition 0.3s background
		transition-property: background, color;
		&[data-file-name]
			font-size: 10px;
		&[data-file-name] span
			display: none;
		&[data-file-name]:before
			content: '\0412\044b\0431\0440\0430\043d\0020\0444\0430\0439\043b\003a';
			margin-right: 5px;
		&[data-file-name]:after
			content: attr(data-file-name);
			font-size: 10px;
		&[data-file-name][data-file-size]:after
			content: attr(data-file-name) " ["attr(data-file-size)attr(data-file-size-label)"]";
	&:hover &-button
		background-color: (MAIN_COLOR + 20%)
	&:active &-button
		background-color: (MAIN_COLOR - 20%)
